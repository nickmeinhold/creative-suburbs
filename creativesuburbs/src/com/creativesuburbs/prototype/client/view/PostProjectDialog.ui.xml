<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
        .panel {
            background-color: ivory;
        }
    </ui:style>
    
    <g:FlowPanel styleName="{style.panel}">
    <g:Label text="I would like help with...">Dialog Content</g:Label>
    <g:HorizontalPanel>
    	<g:VerticalPanel spacing="3">
    	<g:CheckBox ui:field="teamHelpCB">TEAM</g:CheckBox>
    	<g:VerticalPanel ui:field="teamSubList" spacing="5">
    		<g:RadioButton name="new name" text="Street" ui:field="streetSizeRB"/>
    		<g:RadioButton name="new name" text="Neighbourhood" ui:field="neighbourhoodSizeRB"/>
    		<g:RadioButton name="new name" text="City" ui:field="citySizeRB"/>
    	</g:VerticalPanel>
    	<g:CheckBox ui:field="financeHelpCB">FINANCE</g:CheckBox>
    	<g:CheckBox ui:field="govHelpCB">GOV</g:CheckBox>    
    </g:VerticalPanel>
    	<g:VerticalPanel>
    		<g:SimplePanel width="20" height="20">
    	<g:Image width="100%" height="100%" ui:field="image"/>
    </g:SimplePanel>
    		<g:Button text="Add Image" ui:field="addImageButton"/>
    		<g:TextBox ui:field="addImageTB"/>
    	</g:VerticalPanel>
    </g:HorizontalPanel>
    <g:Button text="Continue" ui:field="continueButton"/>
    
    
    </g:FlowPanel>
</ui:UiBinder> 